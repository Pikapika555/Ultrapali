modals
	#regMod.modal.hide.fade(tabindex="-1", role="dialog", aria-labelledby="myModalLabel", aria-hidden="true")
		form.ajaxForm.form-horizontal(method="post", enctype="multipart/form-data", action="registrate")
			.modal-header
				button.close(type="button", data-dismiss="modal", aria-hidden="true") &times;
				h3 Registrieren
			.modal-body
				.control-group
						label.control-label Email
						.controls
							input.span3(type="text", name="R_email", placeholder="Email")
				.control-group
						label.control-label Passwort
						.controls
							input.span3(type="password", name="R_password", placeholder="Passwort")
				.control-group
						label.control-label Passwort
						.controls
							input.span3(type="password", name="R_password2", placeholder="Passwort Wiederholen")
				hr.s
				.tabbable
					.control-group
						.btn-group.ajaxGrp(data-toggle="buttons-radio", data-toggle-name="is_company")
							button.btn.active(type="button", href="#ttab1",value="0", data-toggle="tab", tabindex="-1") Privatperson 
							button.btn(type="button", href="#ttab2",value="1", data-toggle="tab", tabindex="-1") Firma
						input(type="hidden", name="is_company", value="0")
				.control-group
					label.control-label Vorname
					.controls
						input(type="text",name="prename", placeholder="Vorname")
				.control-group
					label.control-label Nachname
					.controls
						input(type="text",name="lastname", placeholder="Nachname")
				.control-group
					label.control-label Straße
					.controls
						.input-append
							input(type="text",name="streetname", placeholder="Straßenname")
						.input-prepend
							input.span1(type="text",name="streetNr", placeholder="Nr")
				.control-group
					label.control-label Stadt
					.controls
						.input-append
							input.span1(type="text",name="plz", placeholder="PLZ")
						.input-prepend
							input(type="text",name="cityname", placeholder="Stadt")
				.control-group
					label.control-label(for="country") Land
					.controls
						input(type="text",name="country", placeholder="Land")
				.tab-content
					.tab-pane(id="ttab1")
					.tab-pane(id="ttab2")
						.control-group
							label.control-label Firmenname
							.controls
								input.toggleInput(type="text",name="firmenname", placeholder="Firmenname", disabled)
						.control-group
							label.control-label Identifikationsnr
							.controls
								input.toggleInput(type="text",name="ust_id", placeholder="USt.-Id/UID/Steuernummer", disabled)
						.control-group
							.controls
								input.toggleInput(name="kleinunternehmer", type="checkbox", disabled)
								|  Ich bin Kleinunternehmer gemäß dem deutschen UStG (§ 19) 
				.control-group
					.controls
						label.span3.checkbox.inline
							input(type="checkbox", name="checkAgb")
							| AGB's akzeptieren
				hr.s
						
			.modal-footer
				button.btn.btn-primary.btn-block(type="submit")
					b Registrieren
				a(href="#agbModal", data-toggle="modal") Lies dir hier die AGB's durch

	#artistModal.modal.hide.fade(tabindex="-1", role="dialog", aria-labelledby="myModalLabel", aria-hidden="true")
		form.form-horizontal.ajaxForm.artistSubmit(enctype="multipart/form-data", method="post", action="addArtist")
			.modal-header
				button.close(type="button", data-dismiss="modal", aria-hidden="true") &times;
				h3 Künstler Hinzufügen
			.modal-body
				input.hidden(name="hid")
				.control-group
					label.control-label * Name :
					.controls
						input(type="text", name="artistName", placeholder="Exakter name wie auf Cover")
				.control-group
					label.control-label Biographie :
					.controls
						textarea.vert(name="biography")
				.control-group
					label.control-label Land :
					.controls
						select(name="country")
							option(value="de") Deutschland
							option(value="en") England
							option(value="fr") France
				.control-group
					label.control-label Website :
					.controls
						input(type="text", name="website", value="http://")
				.control-group
					label.control-label Facebook :
					.controls
						input(type="text", name="facebook", value="http://")
				.control-group
					label.r * = Pflichtfeld
			.modal-footer
				button.btn.btn-primary(button="submit") Save

