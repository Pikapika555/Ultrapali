	#NotiModal.modal.hide.fade(tabindex="-1", role="dialog", aria-labelledby="myModalLabel", aria-hidden="true")
		.modal-header
			button.close(type="button", data-dismiss="modal", aria-hidden="true") &times;
			h3 Request Admin
		form.form-horizontal.ajaxForm(method="POST", accept-charset="UTF-8", action="sendRequest")
			.modal-body
				.control-group
					label.control-label(for="") Anliegen: 
					.controls
						select(name="type")
							option Allgemeiner Fehler
							option Upload Problem
							option Bezahl Problem
							option Anderes Problem
							option Allgemeine Frage
							option Rechtliche Frage
							option Business Partner Anliegen
							option Feedback
				.control-group
					label.control-label(for="") Betreff
					.controls
						input(type="text", name="headline", placeholder="short description")
				.control-group
					textarea.full(name="msg", rows="6")
				
			.modal-footer
				button.btn.btn-primary(type="submit")	Submit

