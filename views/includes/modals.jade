	#messageModal.modal.hide.fade(tabindex="-1", role="dialog", aria-labelledby="myModalLabel", aria-hidden="true")
		form.form-horizontal(enctype="multipart/form-data", method="get", action="delNews")
			.modal-header
				button.close(type="button", data-dismiss="modal", aria-hidden="true") &times;
				h3 Deine Benachrichtigungen
			.modal-body
				-if(navi.email)
					table.table
						thead
							tr
								th Betreff
								th Von
								th Zeit
								th
						tbody
							-for(key in msg.recieved)
								div.collapse(id="read_#{key}")
										.well.d
											h5 #{msg.recieved[key].title}
											hr
											| #{msg.recieved[key].msg}
								tr
									td #{msg.recieved[key].title}
									td #{msg.recieved[key].from}
									td #{msg.recieved[key].time}
									td 
										button.btn.btn-primary(type="button", data-toggle="collapse", data-target="#read_#{key}") Lesen
										a.btn(href="deleteMessage/#{key}") Delete

			.modal-footer
